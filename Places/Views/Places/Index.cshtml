@using Travel.Models;
<h1>Places I have been</h1>
@if (Model.Count == 0)
{
  <p>I am the ultimate homebody</p>
}
<ul>
  @foreach (Place place in Model)
  {
    <li>@place.CityName <form action="places/delete" method="post"><button type="submit" name="button">Delete this place</button></form></li>
  }
</ul>